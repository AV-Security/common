<launch>
 <arg name="file_name"/>	
 <arg name="projector" default="UTM" /> <!-- UTM or MGRS -->
 <arg name="origin_lat" default="36.0" />
 <arg name="origin_lon" default="137.1666666669" /> 
 <node pkg="map_file" type="lanelet2_map_loader" name="lanelet2_map_loader"  args="$(arg file_name)">     
 </node>
 <node pkg="map_file" type="lanelet2_map_visualization" name="lanelet2_map_visualization" />
 <param name="projector" value="$(arg projector)" />
 <param name="origin_lat" value="$(arg origin_lat)" />
 <param name="origin_lon" value="$(arg origin_lon)" />
</launch>